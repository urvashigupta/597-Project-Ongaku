<div class="page-header">
    <div class="info">
        <h2>All music</h2>
        <p class="meta"><span class="count">0</span> <span class="text">songs</span></p>
    </div>
    <form method="post">
    <div class="search" align="right">
        <input id="searching" type="text" placeholder="Find songs" />
        <input type="reset" value="Clear">
		&nbsp&nbsp&nbsp|&nbsp&nbsp&nbsp<%= link_to "Logout?", logout_index_path %>
    </div>
  </form>
	
</div>

<div class="grid-container">
    <div id="slickgrid"></div>
</div>
<br/>
<table>
  <tr>
    <th>Title</th>
    <th>Artist Name</th>
    <th>Duration</th>
    <th>Year</th>
  </tr>
  <% @songs.each do |track| %>
  <tr>
    <td><%= track.title %></td>
    <td><%= track.artist_name %></td>
    <td><%= track.duration %></td>
    <td><%= track.year %></td>
  </tr>
  <%end%>
</table>
<br/>
<br/>
<br/>
<br/>
<div style="position:relative;width:800px;height:25px;overflow:hidden;">
  <div style="position:absolute;top:-276px;left:-5px">
    <iframe width="800" height="300" 
      src="https://www.youtube.com/embed/I-SqFmhBUp4">
    </iframe>
  </div>
</div>
